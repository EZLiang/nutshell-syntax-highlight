{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Nutshell",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
				  "name": "keyword.control.nutshell",
			  	"match": "(@COLORS|@TABLE|@NUTSHELL|@ICONS)"
				},
				{
					"name": "comment.line",
					"match": "# ."
				},
				{
					"name": "entity.name.function",
					"match": "(weave|reorder)"
				},
				{
					"name": "keyword.operator",
					"match": "(\\+|\\*|\\.\\.|-|>|=)"
				}
		  ]
		}
	},
	"scopeName": ""
}